#import "YBDefaultLabel.h"

@implementation YBDefaultLabel

+ (instancetype)labelWithFont:(UIFont *)font
                         text:(NSString *)text
                    textColor:(UIColor *)textColor
{
    NSString *string            = ZJSTRING(text);
    YBDefaultLabel *label       = [super labelWithFont:font
                                                  text:string
                                             textColor:textColor];
    label.backgroundColor       = ZJCOLOR.color_c0;
    return label;
}

@end
