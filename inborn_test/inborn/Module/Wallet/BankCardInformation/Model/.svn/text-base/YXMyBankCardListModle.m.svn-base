#import "YXMyBankCardListModle.h"

@implementation YXMyBankCardListModle

//银行卡类型  card_type  否  String  2-储蓄卡    3-信用卡
-(NSString *)card_type{

    if ([_card_type isEqualToString:@"2"]) {
        return @"储蓄卡";
    }else if ([_card_type isEqualToString:@"3"]) {
        return @"信用卡";
    }
    return nil;
}
@end
